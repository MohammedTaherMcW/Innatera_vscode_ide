      ]
    },
    "viewsWelcome": [
      {
        "view": "Innatera-ide.projectTasks",
        "contents": "You have not yet opened a innatera project.\nYou can open an existing innatera-based project (a folder that contains [innaterapluginio.ini](https://bit.ly/vscode-innatera-ini-docs) file).\n[Pick a folder](command:workbench.action.files.openFolder)\nYou can create a new innatera Project or explore examples using innatera Home.\n[Create New Project](command:Innatera-ide.showHome)"
      },
      {
        "view": "Innatera-ide.quickAccess",
        "contents": "Initializing innatera Core..."
      }
    ],
    "walkthroughs": [
      {
        "id": "Innatera-ide.welcome",
        "title": "Get started with innatera IDE",
        "description": "The most loved IDE solution for Microsoft Visual Studio Code",
        "steps": [
          {
            "id": "innatera-core",
            "title": "Rich CLI developed for Humans",
            "description": "[innatera Core](https://bit.ly/vscode-innatera-core-docs) is the heart of innatera IDE and it's your one-stop shop for professional embedded development.\nIt takes care of everything from dependency management to testing, so you can focus on what matters most ‚Äì your project.\n[Install innatera Core](command:Innatera-ide.openPIOCoreCLI)",
            "media": {
              "image": "assets/welcome/innatera-core-cli-demo.png",
              "altText": "innatera Core CLI Demo"
            },
            "completionEvents": [
              "onContext:InnateraCoreReady"
            ]
          },
          {
            "id": "innaterapluginio.ini",
            "title": "Platform-agnostic shareable configuration",
            "icon": "assets/icons/file_type_innatera.svg",
            "description": "[innaterapluginio.ini](https://bit.ly/vscode-innatera-ini-docs) is the single configuration file that lets you set up your development environment and share your configurations with your team or organization.\nLeverage a declarative approach to embedded development and the power of semantic versioning.\n[Learn More](https://bit.ly/vscode-innatera-ini-docs)",
            "media": {
              "markdown": "assets/welcome/innatera-ini-example.md"
            },
            "completionEvents": [
              "onLink:https://bit.ly/vscode-innatera-ini-docs"
            ]
          },
          {
            "id": "innatera-home",
            "title": "Advanced project management",
            "description": "Boost your product development with [innatera Home](https://bit.ly/vscode-innatera-home-docs) ‚Äì a powerful, modern, and interactive user interface for advanced project management.\nIt allows you to enhance your project source code with advanced instruments such as firmware explorer, memory inspection, and static code analysis.\n[Open Innaera Home](command:Innatera-ide.showHome)\nTip: Use üè† icon at the bottom status bar.",
            "media": {
              "image": "assets/welcome/project-inspection-demo.png",
              "altText": "innatera Project Inspection Demo"
            }
          },
          {
            "id": "explore-more-resources",
            "title": "Explore more resources",
            "description": "üì¶ Browse over 10,000 libraries from software vendors, open-source projects, and the community in the [innatera Registry](https://bit.ly/innatera-registry).\nüöÄ Learn more about getting started with [innatera IDE for VSCode](https://bit.ly/Innatera-ide-for-vscode-docs).\nüß° Have any ideas or need help with your project? Ask our awesome [innatera Community](https://community.innatera.org/).\n[Learn More](https://bit.ly/Innatera-ide-for-vscode-docs)",
            "media": {
              "image": "assets/welcome/innatera-package-registry-demo.png",
              "altText": "innatera Package Registry"
            },
            "completionEvents": [
              "onLink:https://bit.ly/Innatera-ide-for-vscode-docs"
            ]
          },
          {
            "id": "ready-to-start",
            "title": "Ready to start?",
            "description": "Navigate to the innatera IDE View in the left activity bar and open an existing project or create a new one.\n[Open innatera IDE View](command:workbench.view.extension.innatera)",
            "media": {
              "image": "assets/welcome/Innatera-ide-view-demo.png",
              "altText": "innatera IDE View"
            },
            "completionEvents": [
              "onView:Innatera-ide.projectTasks"
            ]
          }
        ]
      }
    ],
    "keybindings": [
      {
        "command": "Innatera-ide.build",
        "key": "ctrl+alt+b",
        "when": "InnateraProjectReady"
      },
      {
        "command": "Innatera-ide.upload",
        "key": "ctrl+alt+u",
        "when": "InnateraProjectReady"
      },
      {
        "command": "Innatera-ide.serialMonitor",
        "key": "ctrl+alt+s",
        "when": "InnateraProjectReady"
      }
    ],
    "taskDefinitions": [
      {
        "type": "innatera",
        "required": [
          "task"
        ],
        "properties": {
          "task": {
            "type": "string",
            "description": "innatera Task ID"
          }
        }
      }
    ],
    "problemMatchers": [
      {
        "name": "innatera",
        "owner": "cpp",
        "fileLocation": [
          "relative",
          "${workspaceFolder}"
        ],
        "pattern": {
          "regexp": "^([^:\\n]+):(\\d+):(\\d+):\\s+(warning|error):\\s+(.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "severity": 4,
          "message": 5
        }
      }
    ],
    "configurationDefaults": {
      "C_Cpp.debugShortcut": false
    },
    "configuration": {
      "type": "object",
      "title": "innatera IDE",
      "properties": {
        "Innatera-ide.activateProjectOnTextEditorChange": {
          "type": "boolean",
          "default": false,
          "description": "Automatically activate project depending on an active opened text editor"
        },
        "Innatera-ide.autoOpeninnateraIniFile": {
          "type": "boolean",
          "default": true,
          "description": "Automatically open the `innaterapluginio.ini` file from a project when no other editors are opened"
        },
        "Innatera-ide.autoCloseSerialMonitor": {
          "type": "boolean",
          "default": true,
          "description": "Automatically close Serial Port Monitor before uploading/testing"
        },
        "Innatera-ide.autoRebuildAutocompleteIndex": {
          "type": "boolean",
          "default": true,
          "description": "Automatically rebuild the project IntelliSense index when innaterapluginio.ini is changed or when new libraries are installed"
        },
        "Innatera-ide.showEditorTitleShortcuts": {
          "type": "boolean",
          "default": true,
          "description": "Show Build, Upload, Clean, and other buttons in the editor title bar"
        },
        "Innatera-ide.autoPreloadEnvTasks": {
          "type": "boolean",
          "default": false,
          "description": "Automatically preload ALL project environment tasks"
        },
        "Innatera-ide.customPATH": {
          "type": [
            "string",
            "null"
          ],
          "default": null,
          "description": "Custom PATH for the `innatera` command, if you prefer to use a custom version of innatera Core. Fill in the result of the system terminal command `echo $PATH` (Unix) / `echo %PATH%` (Windows)."
        },
        "Innatera-ide.reopenSerialMonitorDelay": {
          "type": "integer",
          "default": 0,
          "minimum": 0,
          "description": "Time in milliseconds after which the Serial Port Monitor is reopened"
        },
        "Innatera-ide.useBuiltinPython": {
          "type": "boolean",
          "default": true,
          "description": "Use a portable Python 3 Interpreter if available"
        },
        "Innatera-ide.useBuiltinPIOCore": {
          "type": "boolean",
          "default": true,
          "description": "Use a built-in innatera Core"
        },
        "Innatera-ide.useDevelopmentPIOCore": {
          "type": "boolean",
          "default": false,
          "description": "Use a development version of innatera Core"
        },
        "Innatera-ide.disablePIOHomeStartup": {
          "type": "boolean",
          "default": false,
          "description": "Disable showing PIO Home at startup"
        },
        "Innatera-ide.pioHomeServerHttpHost": {
          "type": "string",
          "default": "127.0.0.1",
          "description": "innatera Home server HTTP host (default is 127.0.0.1, but in case of dockerized environments 0.0.0.0)"
        },
        "Innatera-ide.pioHomeServerHttpPort": {
          "type": "integer",
          "description": "innatera Home server HTTP port (the default value 0 automatically assigns a free port in the range [45000..45999])"
        },
        "Innatera-ide.pioCustomServerHttpHost": {
          "type": "string",
          "default": "127.0.0.2",
          "description": "innatera Custom server HTTP host (default is 127.0.0.2, but in case of dockerized environments 0.0.0.0)"
        },
        "Innatera-ide.pioCustomServerHttpPort": {
          "type": "integer",
          "description": "innatera Custom server HTTP port (the default value 0 automatically assigns a free port in the range [45000..45999])"
        },
        "Innatera-ide.customPyPiIndexUrl": {
          "type": [
            "string",
            "null"
          ],
          "default": null,
          "description": "Custom base URL of the Python Package Index (default `https://pypi.org/simple`)"
        },
        "Innatera-ide.toolbar": {
          "description": "innatera Toolbar",
          "type": "array",
          "items": {
            "type": "object",
            "title": "Button",
            "properties": {
              "text": {
                "type": "string",
                "description": "The text to show for the button",
                "required": true
              },
              "tooltip": {
                "type": "string",
                "description": "The tooltip text when you hover over this button"
              },
              "commands": {
                "description": "The button command or list of commands to execute",
                "anyOf": [
                  {
                    "type": "string",
                    "title": "The command identifier"
                  },
                  {
                    "type": "array",
                    "items": {
                      "type": "string",
                      "title": "The command identifier"
                    }
                  },
                  {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "title": "Command",
                      "properties": {
                        "id": {
                          "type": "string",
                          "title": "The command identifier"
                        },
                        "args": {
                          "type": [
                            "array",
                            "string",
                            "object",
                            "null"
                          ],
                          "description": "The button command arguments",
                          "default": null
                        }
                      }
                    }
                  }
                ]
              },
              "when": {
                "anyof": [
                  {
                    "type": "string"
                  },
                  {
                    "type": "array",
                    "items": {
                      "type": "string",
                      "title": "when",
                      "properties": {
                        "id": {
                          "type": "string",
                          "title": "The command identifier"
                        }
                      }
                    }
                  }
                ],
                "description": "The condition that must be true to show the button"
              }
            }
          },
          "default": [
            {
              "text": "$(home)",
              "tooltip": "innatera: Home",
              "commands": "Innatera-ide.showHome"
            },
            {
              "text": "$(check)",
              "tooltip": "innatera: Python Run",
              "commands": "Innatera-ide.python_run",
              "when": "talamo, combine"
            },
            {
              "text": "$(check)",
              "tooltip": "innatera: Build",
              "commands": "Innatera-ide.build",
              "when": "spine, combine"
            },  
            {
              "text": "$(arrow-right)",
              "tooltip": "Innnatera: Upload",
              "commands": "Innatera-ide.upload",
              "when": "talamo, spine, combine"
            },
            {
              "text": "$(trashcan)",
              "tooltip": "innatera: Clean",
              "commands": "Innatera-ide.clean",
              "when": "talamo, spine, combine"
            },
            {
              "text": "$(beaker)",
              "tooltip": "innatera: Test",
              "commands": "Innatera-ide.test",
              "when": "talamo, spine, combine"
            },
            {
              "text": "$(plug)",
              "tooltip": "innatera: Serial Monitor",
              "commands": "Innatera-ide.serialMonitor",
              "when": "talamo, spine, combine"
            },
            {
              "text": "$(terminal)",
              "tooltip": "innatera: New Terminal",
              "commands": "Innatera-ide.newTerminal",
              "when": "talamo, spine, combine"
            }
          ]
        }
      }
    }
  },
  "scripts": {
    "build": "webpack --mode production",
    "lint": "eslint .eslintrc.js src",
    "format": "prettier --single-quote --print-width 88 --write \"src/**/*.js\"",
    "vscode:package": "webpack --mode production && vsce package"
  },  
  "dependencies": {
    "fs-plus": "~3.1.1",
    "Innatera-node-helpers": "git+https://github.com/MohammedTaherMcW/Innatera_node_modules.git",
    "Innatera-vscode-debug": "git+https://github.com/MohammedTaherMcW/Innatera_debug_adapter.git"
  },  
  "devDependencies": {
    "@types/node": "~14",
    "@types/vscode": "~1.65.0",
    "@vscode/vsce": "~2.23.0",
    "eslint": "~8.56.0",
    "eslint-import-resolver-webpack": "~0.13.8",
    "eslint-plugin-import": "~2.29.1",
    "prettier": "~3.2.4",
    "webpack": "~5.90.0",
    "webpack-cli": "~5.1.4"
  },
  "extensionDependencies": [
    "ms-vscode.cpptools"
  ]

  
}

